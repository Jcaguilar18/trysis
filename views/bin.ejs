<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        .inventory-table {
    width: 100%;
    border-collapse: collapse;
}

.inventory-table th, .inventory-table td {
    border: 1px solid #ddd;
    padding: 8px;
}

.inventory-table th {
    padding-top: 12px;
    padding-bottom: 12px;
    text-align: left;
    background-color: #4CAF50;
    color: white;
}

    </style>
</head>
<body>
    <div class="inventory-table">
        <table>
            <thead>
                <tr>
                    <th>CODE</th>
                    <th>ITEM DESCRIPTION</th>
                    <th>YEAR</th>
                    <th>MONTH</th>
                    <th>BEGINNING INVENTORY</th>
                    <th>TOTAL INCOMING</th>
                    <th>TOTAL OUTGOING</th>
                    <th>AVAILABLE</th>
                    <th>PRICE</th>
                    <th>TOTAL AMOUNT</th>
                </tr>
            </thead>
            <tbody>
                <% items.forEach(item => { %>
                    <tr>
                        <td><%= item.code %></td>
                        <td><%= item.description %></td>
                        <td><%= item.year %></td>
                        <td><%= item.month %></td>
                        <td><%= item.beginning_inventory %></td>
                        <td><%= item.total_incoming %></td>
                        <td><%= item.total_outgoing %></td>
                        <td><%= item.available %></td>
                        <td><%= item.price %></td>
                        <td><%= item.total_amount %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
    <button onclick="window.location.href='/item'">Back</button>
    
</body>
</html>