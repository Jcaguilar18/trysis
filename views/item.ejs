                                <% if (roleOf !== 'warehouse') { %>
                                <button class="action-buttons" onclick="editItemModal('<%= cluster1.clustercode %>', '<%= cluster1.description %>')" style="font-family: FontAwesome;"></button>
                                <% } %>
